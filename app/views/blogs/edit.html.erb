<h1>Blogs#edit</h1>
<p>Find me in app/views/blogs/edit.html.erb</p>
<%= form_with(model: @blog, local: true) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %><br>
    <%= form.label :content %>
    <%= form.text_field :content %><br>
    <%= form.submit "UPDATE"%>
<% end %>
<%= link_to "see all blogs", blogs_path %>