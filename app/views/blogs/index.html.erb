<h1>Blogs#index</h1>
<p>Find me in app/views/blogs/index.html.erb</p>
<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>
<% @blogs.each do |blog| %>
  <tr>
    <td><%= blog.title %></td>
    <td><%= blog.content %></td>
    <td><%= link_to "edit", edit_blog_path(blog.id) %></td>
    <td><%= link_to "delete", blog_path(blog.id), method: :delete %></td>
  </tr>
<% end %>
</table>
<%= link_to 'create new blog', new_blog_path %>